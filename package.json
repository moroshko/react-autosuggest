{
  "name": "react-autosuggest",
  "version": "10.0.0",
  "description": "WAI-ARIA compliant React autosuggest component",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/moroshko/react-autosuggest.git"
  },
  "author": "Misha Moroshko <michael.moroshko@gmail.com>",
  "scripts": {
    "start": "mkdir -p demo/dist && npm run copy-static-files && node server",
    "prettier": "prettier --single-quote --write",
    "prettier:all": "npm run prettier -- \".*.js\" \"*.js\" \"demo/src/**/*.js\" \"demo/standalone/app.js\" \"src/**/*.js\" \"test/**/*.js\"",
    "lint": "eslint src test demo/src demo/standalone/app.js server.js webpack.*.js",
    "test": "cross-env NODE_ENV=test nyc mocha \"test/**/*.test.js\"",
    "copy-static-files": "cp demo/src/index.html demo/src/components/App/components/Examples/components/Basic/autosuggest.css demo/dist/",
    "dist": "rm -rf dist && mkdir dist && babel src -d dist",
    "demo-dist": "rm -rf demo/dist && mkdir demo/dist && npm run copy-static-files && cross-env BABEL_ENV=production webpack --config webpack.gh-pages.config.js",
    "standalone": "cross-env BABEL_ENV=production webpack --config webpack.standalone.config.js && webpack --config webpack.standalone-demo.config.js",
    "prebuild": "npm run prettier && npm run lint && npm test",
    "build": "npm run dist && npm run standalone",
    "gh-pages-build": "npm run prebuild && npm run demo-dist",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run dist && npm run standalone",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "es6-promise": "^4.2.8",
    "prop-types": "^15.7.2",
    "react-autowhatever": "^10.2.1",
    "shallow-equal": "^1.2.1"
  },
  "peerDependencies": {
    "react": ">=16.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "autoprefixer": "^9.7.5",
    "autosuggest-highlight": "^3.1.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "husky": "^4.2.3",
    "ismobilejs": "^1.0.3",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "15.1.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.10",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "openurl": "^1.1.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-transform-hmr": "^1.0.4",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0",
    "style-loader": "^1.1.3",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier"
    ]
  },
  "keywords": [
    "autosuggest",
    "autocomplete",
    "auto-suggest",
    "auto-complete",
    "auto suggest",
    "auto complete",
    "react autosuggest",
    "react autocomplete",
    "react auto-suggest",
    "react auto-complete",
    "react auto suggest",
    "react auto complete",
    "react-autosuggest",
    "react-autocomplete",
    "react-auto-suggest",
    "react-auto-complete",
    "react-component"
  ],
  "license": "MIT"
}
